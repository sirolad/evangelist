{
    "name": "sirolad/open-source-evangelist-agnostic-package",
    "description": "This package aims to provide an analysis based on the number of open source",
    "type": "Project",
    "license": "MIT",
    "authors": [{
        "name": "andela-sakande",
        "email": "surajudeen.akande@andela.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "~4.0",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0",
        "php": ">=5.3.2"
    },
    "autoload": {
        "psr-4": {
            "Sirolad\\Evangelist\\": "src/"
        },
        "classmap": ["src/"]
